# ClubOS Environment Configuration
# Copy these values and update with your actual credentials

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-key-here

# Database Configuration  
DATABASE_URL=postgresql://clubos:your_secure_password@localhost:5432/clubos

# Email Configuration (for ticket notifications)
EMAIL_USER=your-email@gmail.com
EMAIL_PASSWORD=your-16-char-app-password
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
FROM_EMAIL=clubos@yourclubhouse.com

# Application Settings
APP_ENV=production
DEBUG=False

# OpenAI Configuration (Enhanced)
OPENAI_API_KEY=sk-your-actual-key-here

# Google Drive Integration
GOOGLE_SERVICE_ACCOUNT_JSON={"type":"service_account",...}
# OR (for file-based setup)
GOOGLE_SERVICE_ACCOUNT_FILE=service_account.json

# Phone System Integration  
PHONE_SYSTEM_API_URL=https://api.yourphonesystem.com/v1
PHONE_SYSTEM_API_KEY=your-phone-api-key
PHONE_WEBHOOK_SECRET=your-webhook-secret

# Facility Configuration
FACILITY_NAME=Your Golf Facility Name
FACILITY_LOCATION=Your Location
BAY_COUNT=8
